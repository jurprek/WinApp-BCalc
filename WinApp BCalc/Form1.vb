    Private Sub mk15BtnCalculate_Click(sender As System.Object, e As System.EventArgs) Handles mk15BtnCalculate.Click
        mk15BtnCalculate.Visible = False
        mk15OptimumU.Text = ""
        mk15stepU = 0.001
        mk15Y = mk15nmbr
        mk15SumOptiU = 0

        Call DiagonalAprox() '---------------------

        If mk15tempP < mk15P0 Then mk15tempP = mk15P0
        If mk15P2 > mk15tempP Then mk15P2 = mk15P2 Else mk15P2 = mk15tempP

        mk15mKmaxBank.Text = FormatNumber(mk15P2, 5)
        mk15P2 = 0 '----------------------print & clear

        For mk15apFill = 1 To mk15Y
            If mk15u(mk15apFill) = 0 Then mk15u(mk15apFill) = mk15stepU / 10 Else mk15u(mk15apFill) = mk15u(mk15apFill) + mk15stepU
            mk15SumOptiU = mk15SumOptiU + mk15u(mk15apFill)
            mk15OptimumU.Text = mk15OptimumU.Text & FormatNumber(mk15u(mk15apFill), 4) & Environment.NewLine
        Next mk15apFill
        mk15SumOpti.Text = FormatNumber(mk15SumOptiU, 4)

        mk15P0 = 0
        mk15P1 = 0
        mk15P2 = 0
        mk15tempP = 0

        For mk15apFill = 1 To mk15Y
            mk15u(mk15apFill) = mk15TextInputU(mk15apFill)
        Next mk15apFill
        Call EquationB()
        mk15MaxIputedB.Text = FormatNumber(mk15P1, 5)

        mk15P0 = 0
        mk15P1 = 0
        mk15P2 = 0
        mk15tempP = 0

    End Sub

    Private Sub DiagonalAprox()

        ' (1.)  
        mk15SumFill = 0

        For mk15apFill = 1 To mk15Y
            mk15SpointKn(mk15apFill) = (mk15k(mk15apFill) * mk15s(mk15apFill) - 1) / (mk15k(mk15apFill) - 1)
            mk15SumFill = mk15SumFill + mk15SpointKn(mk15apFill)
        Next mk15apFill

        If mk15SumFill < 1 Then GoTo mk15GetOutLAFQuick

        ' (2.)
        For mk15LoopApfill = mk15stepU To mk15SumFill Step mk15stepU
            mk15GetOutLAF = 0
            For mk15apFill = 1 To mk15Y
                mk15SpointKn(mk15apFill) = mk15SpointKn(mk15apFill) - mk15stepU
                If mk15SpointKn(mk15apFill) < 0 Then mk15SpointKn(mk15apFill) = 0
                mk15GetOutLAF = mk15GetOutLAF + mk15SpointKn(mk15apFill)
            Next mk15apFill
            If mk15GetOutLAF <= 1 Then GoTo mk15GetOutLAFQuick
        Next mk15LoopApfill

mk15GetOutLAFQuick:
        For mk15apFill = 1 To mk15Y
            mk15u(mk15apFill) = mk15SpointKn(mk15apFill)
        Next mk15apFill

mk15DAprox3rd:  ' (3.)

        For mk15apFill = 1 To mk15Y
            If mk15u(mk15apFill) < 0 Then mk15u(mk15apFill) = 0 Else mk15u(mk15apFill) = mk15u(mk15apFill) - mk15stepU
        Next mk15apFill

        mk15P0 = mk15P1
        Call EquationB() '-------------------------

        If mk15P0 <= mk15P1 Then GoTo mk15DAprox3rd

    End Sub

    Private Sub EquationB()

        mk15stopCounter = mk15stopCounter + 1

        mk15mKmaxBank.ForeColor = Color.ForestGreen
        mk15MaxIputedB.ForeColor = Color.Maroon
        mk15P1 = 1
        mk15decNo = 0
        For mk15nulS = 0 To 65537
            mk15Suma(mk15nulS) = 0
            mk15Pp(mk15nulS) = 1
        Next mk15nulS '-----------------------end preps; Start equation...

        For mk15a = 0 To mk15Y
            mk15fY = 1
            mk15fa = 1
            mk15fYma = 1

            For mk15FactY = 1 To mk15Y
                mk15fY = mk15fY * mk15FactY
            Next mk15FactY
            For mk15Facta = 1 To mk15a
                mk15fa = mk15fa * mk15Facta
            Next mk15Facta
            For mk15FactYma = 1 To mk15Y - mk15a
                mk15fYma = mk15fYma * mk15FactYma
            Next mk15FactYma

            If mk15fY * mk15fa * mk15fYma = 0 Then mk15C = 1 Else mk15C = mk15fY / mk15fa / mk15fYma

            For mk15d = 1 To mk15C
                mk15sm = Convert.ToString(mk15decNo, 2).PadLeft(mk15Y, "0"c) '"decNo" to bin(2)
                mk15decNo = mk15decNo + 1

                For mk15n = 1 To mk15Y
                    mk15m = mk15sm.ToString().Substring(mk15n - 1, 1)
                    mk15Suma(mk15decNo) = mk15Suma(mk15decNo) + (mk15u(mk15n) * ((1 - mk15m) * mk15k(mk15n) - 1))
                    mk15Pp(mk15decNo) = mk15Pp(mk15decNo) * Math.Abs(mk15s(mk15n) - mk15m)
                Next mk15n
                If (1 + mk15Suma(mk15decNo)) <= 0 Then mk15P1 = 0 Else mk15P1 = mk15P1 * (1 + mk15Suma(mk15decNo)) ^ mk15Pp(mk15decNo)

            Next mk15d
        Next mk15a

        If mk15stopCounter = 1 Then mk15P0 = mk15P1

    End Sub

